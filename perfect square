import java.util.*;
class square{
    public static void main(String[] args) {
        int num;
        Scanner s=new Scanner (System.in);
        System.out.println("Enter the Number: ");
        num=s.nextInt();
        double k=Math.sqrt(num);
        if(k==(int)k){
            System.out.println("perfect square");
        }
        else{
            System.out.println("not a perfect square");
        }
    }
}
